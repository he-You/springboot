<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.heyou.springboot.dao.IUserDao">
    <select id="userList" resultType="com.heyou.springboot.entity.User">
        select
            id userId,
            name userName,
            password
        from user
    </select>
</mapper>
